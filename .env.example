# Copy to .env for local overrides.
# All variables are optional.
# CLI tool selection (lowercase, same as executable name)
YURUTSUKU_TOOL_CLI=codex
YURUTSUKU_TOOL_PATH=
YURUTSUKU_TOOL_ARGS=
YURUTSUKU_TOOL_TIMEOUT_MS=

# Orchestrator health check port (optional; default 17707)
YURUTSUKU_ORCH_HEALTH_PORT=

# terminal-output-broadcast (optional; set to 1 to enable)
YURUTSUKU_ENABLE_TERMINAL_OUTPUT_BROADCAST=
